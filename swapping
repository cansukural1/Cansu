public class swapping {
 
    public static void main(String c[]){
        int a = 10;
        int b = 20;
        System.out.println("Before swap:");
        System.out.println("a value: "+a);
        System.out.println("b value: "+b);
        a = a+b;
        b=a-b;
        a=a-b;
        System.out.println("After swap:");
        System.out.println("a value: "+a);
        System.out.println("b value: "+b);
    }
}
